const musicServices = {
  option1: {
    provider: "Suno AI",
    pros: ["Ã–zel mÃ¼zik Ã¼retir", "Telif yok", "Ucuz"],
    cons: ["API yavaÅŸ", "Kalite deÄŸiÅŸken"],
    cost: "$0.50/song"
  },
  
  option2: {
    provider: "Mubert API",
    pros: ["Real-time generation", "Lofi Ã¶zel", "API hÄ±zlÄ±"],
    cons: ["AylÄ±k Ã¼cretli"],
    cost: "$29/ay (sÄ±nÄ±rsÄ±z)"
  },
  
  option3: {
    provider: "YouTube Audio Library + Remix",
    pros: ["Ãœcretsiz", "Telif sorun yok", "BÃ¼yÃ¼k katalog"],
    cons: ["Manuel kÃ¼rasyon gerekir"],
    cost: "$0"
  },
  
  // Ã–NERÄ°: Hybrid yaklaÅŸÄ±m
  recommended: {
    mvp: "YouTube Audio Library", // Ä°lk 3 ay
    v1_5: "Mubert API", // Ãœcretli kullanÄ±cÄ±lar iÃ§in
    v2_0: "Suno AI custom tracks" // Premium Ã¶zellik
  }
};

// MÃ¼zik SeÃ§im AkÄ±ÅŸÄ±
async function selectSessionMusic(roomParticipants) {
  // 1. KatÄ±lÄ±mcÄ± tercihlerini topla
  const preferences = await Promise.all(
    roomParticipants.map(p => getUserMusicPreference(p.id))
  );
  
  // 2. Dominant mood belirle
  const moodCounts = preferences.reduce((acc, pref) => {
    acc[pref.mood] = (acc[pref.mood] || 0) + 1;
    return acc;
  }, {});
  
  const dominantMood = Object.keys(moodCounts).sort(
    (a, b) => moodCounts[b] - moodCounts[a]
  )[0];
  
  // 3. AI'dan mÃ¼zik iste (v1.5+) veya playlist seÃ§ (MVP)
  if (isPremiumRoom) {
    const track = await generateAIMusic({
      mood: dominantMood,
      duration: 50, // dk
      genre: "lofi",
      bpm: "70-90",
      instruments: ["piano", "bass", "light_drums"],
      vibe: preferences[0].theme // Tema uyumlu
    });
    
    return track;
  } else {
    // Free tier: Pre-curated playlist
    return selectFromPlaylist(dominantMood);
  }
}
```

