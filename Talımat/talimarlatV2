    level: 2,
    restrictions: {
      canMatchWith: ["all"],
      dailySessionLimit: null, // SÄ±nÄ±rsÄ±z
      canReportOthers: true,
      canBeReported: false // Ä°mmunity
    },
    requirements: {
      completedSessions: 20,
      trustScore: 100,
